<!--pages/userInfo/userInfo.wxml-->
<view class='container'>
<view class="userinfo">
    <button open-type="getUserInfo" bindgetuserinfo="onGetUserInfo" class="userinfo-avatar" bindtap='Userinfo' style="background-image: url({{avatarUrl}})"></button>
    
    <view>
      <button class="userinfo-tequan">特权等级V11></button>
    </view>
  </view>
  <view class='uploader'>
    <text class='list'>昵称</text>
    <text class='righttest'></text>
 
  </view>
  <view class='uploader'>
    <text class='list'>性别</text>
    <text class='righttest'></text>
 
  </view>
  <view class='uploader'>
    <text class='list'>等级</text>
    <text class='righttest'></text>
 
  </view>
</view>
<view>
    <button class='btn-class' bindtap='userInfoModify'>修改</button>
  </view>
