.webview{
  width: 100%;
  height: 100%;
}
.logoview{
  text-align: center;
}
.loginview{
  text-align: center;
  padding-left: 25%;
  padding-right: 25%;
  margin-top: 10px;
}
.slidersBox {
  width: 80%;
  height: 60rpx;
  border: 1px solid #ccc;
  border-radius: 5px;
  margin: 0 auto;
  text-align: center;
  line-height: 60rpx;
  font-size: 25rpx;
  position: relative;
  overflow: hidden;
}

.operationBgBox {
  height: 60rpx;
  width: 100%;
  background-color: #7BBB55;
  border-radius: 5px;
  overflow: hidden;
  position: absolute;
  top: 0;
  color: #fff;
}

.operationBox {
  height: 60rpx;
  width: 50px;
  background-color:#EBEBEB;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 0;
  top: 0;
}

.operationBox image {
  width: 50%;
  height: 50%;
}