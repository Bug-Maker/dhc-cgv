    <input name='beginCity' value='{{begin}}' disabled='disabled' placeholder="定位城市" bindtap='bindBeginCityView' style='width:160rpx' />
    <icon class='iconfont icon-xiajiantou icon-right' bindtap='bindBeginCityView'></icon>
  </view>
  <view class='item-left flex-wrp-row'>
    <text class='medium-font-size'></text>
  </view>
</view>

<import src='../../templates/jumpcinema.wxml' />
<view wx:for="{{movieAddressList}}" wx:key="{{movieAddressList}}">
  <template is='jpCinemaTemplate' data="{{...item}}" />
</view>
